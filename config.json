{
  "side_containers": {
    "hermes": {
      "image": "fdom93/hermes:0.0.1"
    },
    "docker-utils": {
      "image": "fdom93/docker-utils:0.0.1",
      "volumes": [
        "/var/run/docker.sock:/var/run/docker.sock",
        "/home/federico/.docker/config.json:/root/.docker/config.json",
        "/var/opt/loko/volumes/docker-utils-ext/mount/templates:/templates"
        ],
      "environment": {
        "ENGINE": "docker",
        "SOCKET_HOST": "hermes",
        "SOCKET_PORT": "8080"
      },
      "ports": {
        "8080": "8400"
      },
      "expose": [
        8080
      ],
      "gui": {
        "name": "Docker Utils Dashboard",
        "path": "/",
        "gw": false
      }
    }
  }
}
